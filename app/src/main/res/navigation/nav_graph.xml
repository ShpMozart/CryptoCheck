<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    tools:ignore="UnusedNavigation"
    app:startDestination="@id/title2">

    <fragment
        android:id="@+id/title2"
        android:name="com.example.woc.title.title"
        android:label="fragment_title"
        tools:layout="@layout/fragment_title" >
        <action
            android:id="@+id/action_title2_to_btc"
            app:destination="@id/btc" />
        <action
            android:id="@+id/action_title2_to_eth"
            app:destination="@id/eth" />
        <action
            android:id="@+id/action_title2_to_xrp"
            app:destination="@id/xrp" />
    </fragment>
    <fragment
        android:id="@+id/btc"
        android:name="com.example.woc.btc.btc"
        android:label="fragment_btc"
        tools:layout="@layout/fragment_btc" />
    <fragment
        android:id="@+id/eth"
        android:name="com.example.woc.eth.eth"
        android:label="fragment_eth"
        tools:layout="@layout/fragment_eth" />
    <fragment
        android:id="@+id/xrp"
        android:name="com.example.woc.xrp.xrp"
        android:label="fragment_xrp"
        tools:layout="@layout/fragment_xrp" />
</navigation>